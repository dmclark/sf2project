<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="JRI\CatsBundle\Entity\Question" table="question">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="text" column="name" nullable="true"/>
    <field name="type" type="string" column="type" length="255" nullable="true"/>
    <field name="instrumentId" type="bigint" column="instrument_id" nullable="false"/>
    <field name="itemNumber" type="string" column="item_number" length="10" nullable="false"/>
    <field name="sortKey" type="float" column="sort_key" nullable="false"/>
    <field name="fieldset" type="string" column="fieldset" length="10" nullable="true"/>
    <field name="isRequired" type="boolean" column="is_required" nullable="false"/>
    <field name="choice" type="string" column="choice" length="30" nullable="true"/>
    <field name="choiceArray" type="text" column="choice_array" nullable="true"/>
    <field name="isItemNumberPrinted" type="boolean" column="is_item_number_printed" nullable="true"/>
    <field name="isNamePrinted" type="boolean" column="is_name_printed" nullable="true"/>
    <field name="questionDepth" type="bigint" column="question_depth" nullable="true"/>
    <field name="choiceKeys" type="text" column="choice_keys" nullable="true"/>
  </entity>
</doctrine-mapping>
