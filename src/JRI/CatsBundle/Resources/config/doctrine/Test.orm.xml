<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="JRI\CatsBundle\Entity\Test" table="test">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="clientId" type="bigint" column="client_id" nullable="false"/>
    <field name="staffId" type="bigint" column="staff_id" nullable="false"/>
    <field name="instrumentId" type="bigint" column="instrument_id" nullable="false"/>
    <field name="administeredOn" type="date" column="administered_on" nullable="false"/>
    <field name="lockDate" type="date" column="lock_date" nullable="true"/>
    <field name="quarter" type="integer" column="quarter" nullable="false"/>
    <field name="numRequiredCompleted" type="bigint" column="num_required_completed" nullable="false"/>
    <field name="isComplete" type="boolean" column="is_complete" nullable="false"/>
    <field name="completionStatus" type="bigint" column="completion_status" nullable="true"/>
    <field name="completionExplanation" type="text" column="completion_explanation" nullable="true"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false"/>
  </entity>
</doctrine-mapping>
