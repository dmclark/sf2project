<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="JRI\CatsBundle\Entity\SfGuardUser" table="sf_guard_user">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstName" type="string" column="first_name" length="255" nullable="true"/>
    <field name="lastName" type="string" column="last_name" length="255" nullable="true"/>
    <field name="emailAddress" type="string" column="email_address" length="255" nullable="false"/>
    <field name="username" type="string" column="username" length="128" nullable="false"/>
    <field name="programId" type="integer" column="program_id" nullable="false"/>
    <field name="algorithm" type="string" column="algorithm" length="128" nullable="false"/>
    <field name="salt" type="string" column="salt" length="128" nullable="true"/>
    <field name="password" type="string" column="password" length="128" nullable="true"/>
    <field name="isActive" type="boolean" column="is_active" nullable="true"/>
    <field name="isSuperAdmin" type="boolean" column="is_super_admin" nullable="true"/>
    <field name="lastLogin" type="datetime" column="last_login" nullable="true"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false"/>
  </entity>
</doctrine-mapping>
