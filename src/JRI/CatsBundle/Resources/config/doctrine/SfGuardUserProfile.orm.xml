<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="JRI\CatsBundle\Entity\SfGuardUserProfile" table="sf_guard_user_profile">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="bigint" column="user_id" nullable="false"/>
    <field name="emailNew" type="string" column="email_new" length="255" nullable="true"/>
    <field name="firstname" type="string" column="firstname" length="255" nullable="true"/>
    <field name="lastname" type="string" column="lastname" length="255" nullable="true"/>
    <field name="validateAt" type="datetime" column="validate_at" nullable="true"/>
    <field name="validate" type="string" column="validate" length="33" nullable="true"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false"/>
    <field name="type" type="string" column="type" length="255" nullable="true"/>
    <field name="programId" type="bigint" column="program_id" nullable="false"/>
    <field name="isActive" type="boolean" column="is_active" nullable="false"/>
  </entity>
</doctrine-mapping>
