<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="JRI\CatsBundle\Entity\Program" table="program">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false"/>
    <field name="address1" type="string" column="address1" length="30" nullable="true"/>
    <field name="address2" type="string" column="address2" length="30" nullable="true"/>
    <field name="city" type="string" column="city" length="30" nullable="true"/>
    <field name="state" type="string" column="state" length="2" nullable="true"/>
    <field name="zip" type="string" column="zip" length="12" nullable="true"/>
    <field name="typeId" type="bigint" column="type_id" nullable="false"/>
    <field name="pp" type="integer" column="pp" nullable="false"/>
    <field name="packageId" type="bigint" column="package_id" nullable="false"/>
    <field name="notes" type="text" column="notes" nullable="true"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false"/>
  </entity>
</doctrine-mapping>
