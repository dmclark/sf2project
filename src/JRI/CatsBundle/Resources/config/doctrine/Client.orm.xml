<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="JRI\CatsBundle\Entity\Client" table="client">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="programId" type="bigint" column="program_id" nullable="false"/>
    <field name="fname" type="string" column="fname" length="25" nullable="false"/>
    <field name="lname" type="string" column="lname" length="25" nullable="false"/>
    <field name="dob" type="date" column="dob" nullable="false"/>
    <field name="clinicianId" type="bigint" column="clinician_id" nullable="false"/>
    <field name="genderId" type="boolean" column="gender_id" nullable="false"/>
    <field name="isActive" type="boolean" column="is_active" nullable="false"/>
    <field name="admissionDate" type="date" column="admission_date" nullable="false"/>
    <field name="initiateDate" type="date" column="initiate_date" nullable="false"/>
    <field name="dischargeDate" type="date" column="discharge_date" nullable="true"/>
    <field name="editedBy" type="bigint" column="edited_by" nullable="false"/>
    <field name="version" type="bigint" column="version" nullable="true"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false"/>
    <field name="deletedAt" type="datetime" column="deleted_at" nullable="true"/>
  </entity>
</doctrine-mapping>
